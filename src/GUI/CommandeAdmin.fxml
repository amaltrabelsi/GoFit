<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.CommandeAdminController">
   <children>
      <Label layoutX="198.0" layoutY="34.0" prefHeight="17.0" prefWidth="187.0" text="Les Commandes">
         <font>
            <Font name="Bell MT Bold" size="24.0" />
         </font>
         <textFill>
            <LinearGradient endX="1.0" endY="1.0" startX="0.7630331753554502">
               <stops>
                  <Stop>
                     <color>
                        <Color red="0.6315789222717285" green="0.3354775905609131" blue="0.05847953259944916" />
                     </color>
                  </Stop>
                  <Stop offset="1.0">
                     <color>
                        <Color red="1.0" green="1.0" blue="1.0" />
                     </color>
                  </Stop>
               </stops>
            </LinearGradient>
         </textFill>
      </Label>
      <TableView fx:id="commandetable" layoutX="199.0" layoutY="73.0" prefHeight="272.0" prefWidth="373.0">
        <columns>
          <TableColumn fx:id="comcol" prefWidth="75.0" text="Commande" />
          <TableColumn fx:id="nbprodcol" prefWidth="75.0" text="NbProd" />
            <TableColumn fx:id="modecol" prefWidth="56.0" text="Mode Paiement" />
            <TableColumn fx:id="totalcol" prefWidth="87.0" text="Total" />
            <TableColumn fx:id="datecol" prefWidth="87.0" text="Date" />
        </columns>
      </TableView>
      <Button fx:id="menuid" layoutX="525.0" layoutY="356.0" mnemonicParsing="false" onAction="#gotomenu" text="Menu" />
      <Button fx:id="modifier" layoutX="21.0" layoutY="320.0" mnemonicParsing="false" onAction="#modifier" text="Modifier Commande" />
      <TextField fx:id="modefield" layoutX="21.0" layoutY="188.0" promptText="cash/carte/.." />
      <TextField fx:id="totalfield" layoutX="21.0" layoutY="262.0" promptText="donner le nv total 0.0" />
      <TextField fx:id="nbfield" layoutX="21.0" layoutY="111.0" promptText="entrer un nb valide" />
      <Label layoutX="21.0" layoutY="81.0" prefHeight="17.0" prefWidth="126.0" text="Nombre De Produits:" />
      <Label layoutX="21.0" layoutY="155.0" prefHeight="17.0" prefWidth="126.0" text="Mode de paiement:" />
      <Label layoutX="21.0" layoutY="232.0" prefHeight="17.0" prefWidth="126.0" text="Total" />
      <Button fx:id="supprimer" layoutX="21.0" layoutY="356.0" mnemonicParsing="false" onAction="#supprimer" text="Supprimer Commande" />
   </children>
</AnchorPane>
